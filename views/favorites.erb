<h2> 
  My Favorites
  <a class="right" href="/artists"><button>All Artists</button></a> 
</h2>
<hr>
<ul>
  <% if @favorites.empty? %>
    <%= @favorites %>
  <% else %>
    <% @favorites.each do |favorite| %>
      <li><a href=<%= favorite[:url] %>><%= favorite[:long_title] %></a></li>
    <% end %>
  <% end %>
</ul>

